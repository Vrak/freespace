<p id="notice"><%= notice %></p>

<h1>Members</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Nickname</th>
      <th>Email</th>
      <th>Emergency contact</th>
      <th>Emerg phone</th>
      <th>Med history</th>
      <th>Dependant</th>
      <th>Vehicle licence</th>
      <th>Agrees waiver</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.first_name %></td>
        <td><%= member.last_name %></td>
        <td><%= member.nickname %></td>
        <td><%= member.email %></td>
        <td><%= member.emergency_contact %></td>
        <td><%= member.emerg_phone %></td>
        <td><%= member.med_history %></td>
        <td><%= member.dependant %></td>
        <td><%= member.vehicle_licence %></td>
        <td><%= member.agrees_waiver %></td>
        <td><%= link_to 'Show', member %></td>
        <td><%= link_to 'Edit', edit_member_path(member) %></td>
        <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

 <i class="fi-torsos-all-female"> <%= link_to 'New Member', new_member_path %> </i>
